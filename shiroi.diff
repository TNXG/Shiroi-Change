+      <m.div
+        variants={shouldReduceMotion ? {} : contentVariants}
+        initial="initial"
+        animate="animate"
+        className="mt-12 grid gap-12 md:grid-cols-2"
+      >
+        <DonationSection />
+        <AcknowledgmentSection />
+      </m.div>
+      <m.div
+        variants={shouldReduceMotion ? {} : contentVariants}
+        initial="initial"
+        animate="animate"
+        className="mt-12"
+      >
+        <FunCorner />
+      </m.div>
+    </>
+  )
+}
