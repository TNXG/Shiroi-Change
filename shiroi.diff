+// 添加监听器 / AddEventListener
+const ServiceWoker_Version = '25.04.12';
+const CACHE_NAME = 'TNXG_BLOG_CACHE';
+const IMAGE_CACHE_NAME = 'TNXG_IMAGE_CACHE';
+const IMAGE_CACHE_DURATION = 48 * 60 * 60 * 1000; // 48小时 in milliseconds
+let cachelist = [];
+
+console.log(`
+Send with love by @TNXG
+Developed by @TNXG
+
+TNXG ServiceWorker Version ${ServiceWoker_Version}
+`);
+
+self.addEventListener('install', async function (installEvent) {
+    self.skipWaiting();
+    installEvent.waitUntil(
+        Promise.all([
+            caches.open(CACHE_NAME),
+            caches.open(IMAGE_CACHE_NAME)
+        ]).then(function ([cache, imageCache]) {
+            console.log('[TNXG_SW] Opened caches');
+            return cache.addAll(cachelist);
+        })
+    );
+});
+
+self.addEventListener('fetch', async event => {
+    event.respondWith(handle(event.request));
+});
+
+const fetchOther = async (req) => {
+    const urlStr = req.url;
+    const urlObj = new URL(urlStr);
+    const urlPath = urlObj.pathname;
+
+    // 检查是否是图片请求
+    const isImageRequest = /\.(jpg|jpeg|png|gif|webp|svg|ico|bmp)$/i.test(urlPath);
+
+    if (isImageRequest) {
+        console.log(`[TNXG_SW] 图片请求: ${urlStr}`);
+
+        const cachedImage = await caches.match(req);
+        if (cachedImage) {
+            // Fix: Handle missing/malformed date header
+            let cachedTime;
+            try {
+                const dateHeader = cachedImage.headers.get('date');
+                cachedTime = dateHeader ? new Date(dateHeader).getTime() : 0;
+            } catch (e) {
+                cachedTime = 0;
+            }
+
+            const currentTime = Date.now();
+            const timeLeft = IMAGE_CACHE_DURATION - (currentTime - cachedTime);
+
+            if (cachedTime > 0 && timeLeft > 0) {
+                console.log(`[TNXG_SW] 使用缓存图片:
+  缓存时间: ${new Date(cachedTime).toLocaleString()}
+  剩余缓存时间: ${Math.floor(timeLeft / (60 * 60 * 1000))}小时`);
+                return cachedImage;
+            }
+
+            console.log('[TNXG_SW] 缓存已过期或无效，重新获取');
+        }
+
+        // Fetch and cache new image
+        try {
+            const response = await fetch(req);
+            if (response.ok) {
+                const cache = await caches.open(IMAGE_CACHE_NAME);
+
+                // Clone response immediately for caching
+                const responseToCache = response.clone();
+
+                // Create new headers with proper Date
+                const headers = new Headers(response.headers);
+                headers.set('date', new Date().toUTCString());
+
+                // Get blob data for logging and caching
+                const blob = await response.blob();
+                console.log(`[TNXG_SW] 已缓存新图片:
+  URL: ${urlStr}
+  类型: ${blob.type}
+  大小: ${(blob.size / 1024).toFixed(2)} KB
+  过期时间: ${new Date(Date.now() + IMAGE_CACHE_DURATION).toLocaleString()}`);
+
+                // Create new response for caching
+                const cachedResponse = new Response(blob, {
+                    status: response.status,
+                    statusText: response.statusText,
+                    headers: headers
+                });
+
+                // Put the cached response in cache
+                await cache.put(req, cachedResponse);
+
+                // Return the original response (not the blob one)
+                return responseToCache;
+            }
+            return response;
+        } catch (e) {
+            console.error('[TNXG_SW] 获取图片失败:', e);
+            if (cachedImage) return cachedImage;
+            throw e;
+        }
+    }
+
+    return fetch(req);
+}
+
+const handle = async (req) => {
+    const urlStr = req.url;
+    const urlObj = new URL(urlStr);
+    const urlPath = urlObj.pathname;
+    const domain = urlObj.hostname;
+
+    // Database functions
+    self.db = {
+        read: (key, config) => {
+            if (!config) {
+                config = { type: "text" };
+            }
+            return new Promise((resolve, reject) => {
+                caches.open(CACHE_NAME).then(cache => {
+                    cache.match(new Request(`https://LOCALCACHE/${encodeURIComponent(key)}`))
+                        .then(function (res) {
+                            if (!res) resolve(null);
+                            res.text().then(text => resolve(text));
+                        })
+                        .catch(() => {
+                            resolve(null);
+                        });
+                });
+            });
+        },
+        write: (key, value) => {
+            return new Promise((resolve, reject) => {
+                caches.open(CACHE_NAME).then(function (cache) {
+                    cache.put(new Request(`https://LOCALCACHE/${encodeURIComponent(key)}`), new Response(value));
+                    resolve();
+                }).catch(() => {
+                    reject();
+                });
+            });
+        }
+    };
+
+    // 对数组内所有地址进行请求，返回第一个成功的请求结果并打断其他请求
+    const concurrentRequest = async (urls) => {
+        let controller = new AbortController();
+        const PauseProgress = async (res) => {
+            return new Response(await (res).arrayBuffer(), { status: res.status, headers: res.headers });
+        };
+
+        if (!Promise.any) {
+            Promise.any = function (promises) {
+                return new Promise((resolve, reject) => {
+                    promises = Array.isArray(promises) ? promises : [];
+                    let len = promises.length;
+                    let errs = [];
+                    if (len === 0) return reject(new AggregateError('All promises were rejected'));
+                    promises.forEach((promise) => {
+                        promise.then(value => {
+                            resolve(value);
+                        }, err => {
+                            len--;
+                            errs.push(err);
+                            if (len === 0) {
+                                reject(new AggregateError(errs));
+                            }
+                        });
+                    });
+                });
+            };
+        }
+
+        return Promise.any(urls.map(urls => {
+            return new Promise((resolve, reject) => {
+                fetch(urls, {
+                    signal: controller.signal
+                })
+                    .then(PauseProgress)
+                    .then(res => {
+                        if (res.status == 200 || res.status == 304 || res.status == 404) {
+                            controller.abort();
+                            resolve(res);
+                        } else {
+                            reject(res);
+                        }
+                    })
+                    .catch(e => reject(e));
+            });
+        }));
+    };
+
+    // 处理 SW 特定请求
+    if (urlStr.includes('/sw-req/')) {
+        console.log('[TNXG_SW]检测到SW请求：' + urlStr);
+
+        if (urlStr.includes('/api?getip')) {
+            return fetch('https://api.ip.sb/geoip');
+        }
+
+        if (urlStr.includes('/api?version')) {
+            return new Response(
+                JSON.stringify({
+                    code: 200,
+                    msg: "TNXG_SW",
+                    version: ServiceWoker_Version
+                }),
+                {
+                    headers: {
+                        'Content-Type': 'application/json',
+                    },
+                }
+            );
+        }
+
+        return new Response('{"code":200,"msg":"TNXG_SW"}', {
+            headers: {
+                'Content-Type': 'application/json',
+            },
+        });
+    }
+
+    // 拦截特定请求并处理
+    if (urlStr.includes('mx.tnxg.top')) {
+        const path = urlStr.replace(/(https|http)?:\/\/(.[^/]+)/, '');
+        const acceptStatus = path.includes('/comments/ref/') ||
+            path.includes('/activity/last-year/publication') ||
+            path.includes('/activity/recent') ||
+            path.includes('/recently') ||
+            path.includes('/aggregate/timeline') ||
+            path.includes('/links/all') ||
+            path.includes('/projects/all') ||
+            path.includes('/says') ||
+            path.includes('/notes/nid/') ||
+            path.includes('/notes/list/') ||
+            path.includes('/posts');
+
+        if (acceptStatus && req.method == 'GET') {
+            const mirrorSources = [`https://mx.tnxg.top`, `https://tnxg-sctes.qcxz.ink`].map(url => url + path);
+            return concurrentRequest(mirrorSources);
+        }
+        return fetch(req);
+    }
+
+    // 处理特定域名的请求
+    if (urlStr.includes('hdslb.com')) {
+        if (urlStr.includes('HarmonyOS')) {
+            return fetch(req);
+        }
+
+        const path = urlStr.replace(/(https|http)?:\/\/(.[^/]+)/, '');
+        const mirrorSources = [
+            `https://i0.hdslb.com`,
+            `https://i1.hdslb.com`,
+            `https://i2.hdslb.com`,
+            `https://s1.hdslb.com`,
+            `https://s2.hdslb.com`,
+            `https://s3.hdslb.com`,
+        ].map(url => url + path);
+
+        return concurrentRequest(mirrorSources);
+    }
+
+    // Shiro 绕过图片代理处理函数
+    if (urlObj.pathname.includes("/_next/image")) {
+        const imgurl = decodeURIComponent(urlObj.searchParams.get('url'));
+        console.log('[TNXG_SW]检测到Shiro图片代理链接，转换为直链：' + imgurl);
+        return fetchOther(new Request(imgurl));
+    }
+
+    return fetch(req);
+};
