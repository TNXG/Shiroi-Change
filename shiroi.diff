@@ -0,0 +1,193 @@
+'use client'
+
+import { Icon } from '@iconify/react'
+import type { Variants } from 'motion/react'
+import { AnimatePresence, m } from 'motion/react'
+import { useEffect, useState } from 'react'
+import * as React from 'react'
+
+import { Skeleton } from '~/components/ui/skeleton'
+import { useReducedMotion } from '~/hooks/common/useReducedMotion'
+
+async function fetchSponsorsData() {
+  const response = await fetch('/api/sponsors')
+  const res: sponsors[] = await response.json()
+  return res
+}
+
+export default function AcknowledgmentSection() {
+  const [donors, setDonors] = useState<sponsors[]>([]) // 用来存储捐赠者数据
+  const [currentPage, setCurrentPage] = useState(1)
+  const [isLoading, setIsLoading] = useState(true) // 添加加载状态
+
+  const donorsPerPage = 3
+
+  // 从 API 加载数据
+  useEffect(() => {
+    const loadDonors = async () => {
+      const data = await fetchSponsorsData()
+      setDonors(
+        data.sort(
+          (a, b) => new Date(b.time).getTime() - new Date(a.time).getTime(),
+        ),
+      )
+      setIsLoading(false) // 数据加载完成，更新加载状态
+    }
+
+    loadDonors()
+  }, [])
+
+  const totalPages = Math.ceil(donors.length / donorsPerPage)
+
+  const indexOfLastDonor = currentPage * donorsPerPage
+  const indexOfFirstDonor = indexOfLastDonor - donorsPerPage
+  const currentDonors = donors.slice(indexOfFirstDonor, indexOfLastDonor)
+
+  const shouldReduceMotion = useReducedMotion()
+
+  const containerVariants: Variants = {
+    initial: { opacity: 0 },
+    animate: {
+      opacity: 1,
+      transition: {
+        duration: 0.5,
+        staggerChildren: 0.1,
+      },
+    },
+  }
+
+  const itemVariants: Variants = {
+    initial: { opacity: 0, y: 20 },
+    animate: {
+      opacity: 1,
+      y: 0,
+      transition: { duration: 0.3, ease: 'easeOut' },
+    },
+  }
+  const getChannelIcon = (channel: string): React.ReactElement => {
+    switch (channel) {
+      case '微信支付': {
+        return (
+          <div className="flex items-center">
+            <Icon icon="mingcute:wechat-pay-line" className="ml-2 text-lg" />
+            <span className="ml-2">微信支付</span>
+          </div>
+        )
+      }
+      case '支付宝': {
+        return (
+          <div className="flex items-center">
+            <Icon icon="mingcute:alipay-line" className="ml-2 text-lg" />
+            <span className="ml-2">支付宝</span>
+          </div>
+        )
+      }
+      case 'PayPal': {
+        return (
+          <div className="flex items-center">
+            <Icon icon="mingcute:paypal-fill" className="ml-2 text-lg" />
+            <span className="ml-2">PayPal</span>
+          </div>
+        )
+      }
+      default: {
+        return (
+          <div className="flex items-center">
+            <span className="ml-2">{channel}</span>
+          </div>
+        )
+      }
+    }
+  }
+
+  return (
+    <m.div
+      variants={shouldReduceMotion ? {} : containerVariants}
+      initial="initial"
+      animate="animate"
+      className="rounded-lg bg-white p-6 shadow-lg dark:bg-gray-800"
+    >
+      <m.h2
+        variants={shouldReduceMotion ? {} : itemVariants}
+        className="mb-4 text-2xl font-semibold text-gray-900 dark:text-white"
+      >
+        感谢捐赠者
+      </m.h2>
+      <m.ul
+        variants={shouldReduceMotion ? {} : containerVariants}
+        className="mb-4 h-[400px] space-y-4 overflow-y-auto scrollbar-thin scrollbar-track-gray-100 scrollbar-thumb-gray-300 dark:scrollbar-track-gray-700 dark:scrollbar-thumb-gray-500"
+      >
+        <AnimatePresence mode="wait">
+          {isLoading ? (
+            // 显示骨架屏
+            <Skeleton className="mb-4 h-12 w-full" />
+          ) : (
+            currentDonors.map((donor) => (
+              <m.li
+                key={donor.name + donor.time}
+                variants={shouldReduceMotion ? {} : itemVariants}
+                initial="initial"
+                animate="animate"
+                exit="initial"
+                className="flex flex-col space-y-2 rounded-md bg-gray-50 p-3 dark:bg-gray-700"
+              >
+                <div className="flex items-center justify-between">
+                  <p className="font-medium text-gray-900 dark:text-white">
+                    {donor.name}
+                  </p>
+                  <p className="text-sm text-gray-500 dark:text-gray-400">
+                    {donor.time}
+                  </p>
+                </div>
+                <p className="text-sm text-gray-500 dark:text-gray-400">
+                  捐赠 ¥{donor.amount}
+                </p>
+                {donor.message && (
+                  <p className="text-sm text-gray-600 dark:text-gray-300">
+                    {donor.message}
+                  </p>
+                )}
+                <div className="flex items-center text-sm text-gray-500 dark:text-gray-400">
+                  支付渠道:
+                  {getChannelIcon(donor.channel) || (
+                    <span className="ml-2 font-medium text-gray-900 dark:text-white">
+                      {donor.channel}
+                    </span>
+                  )}
+                </div>
+              </m.li>
+            ))
+          )}
+        </AnimatePresence>
+      </m.ul>
+      <m.div
+        variants={shouldReduceMotion ? {} : itemVariants}
+        className="mt-4 flex items-center justify-between"
+      >
+        <m.button
+          whileHover={shouldReduceMotion ? {} : { scale: 1.05 }}
+          whileTap={shouldReduceMotion ? {} : { scale: 0.95 }}
+          onClick={() => setCurrentPage((prev) => Math.max(prev - 1, 1))}
+          disabled={currentPage === 1}
+          className="rounded-md border border-gray-300 bg-white px-4 py-2 text-sm font-medium text-gray-700 hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-indigo-500 focus:ring-offset-2 disabled:opacity-50 dark:border-gray-600 dark:bg-gray-800 dark:text-gray-200 dark:hover:bg-gray-700"
+        >
+          上一页
+        </m.button>
+        <span className="text-sm text-gray-600 dark:text-gray-400">
+          第 {currentPage} 页，共 {totalPages} 页
+        </span>
+        <m.button
+          whileHover={shouldReduceMotion ? {} : { scale: 1.05 }}
+          whileTap={shouldReduceMotion ? {} : { scale: 0.95 }}
+          onClick={() =>
+            setCurrentPage((prev) => Math.min(prev + 1, totalPages))
+          }
+          disabled={currentPage === totalPages}
+          className="rounded-md border border-gray-300 bg-white px-4 py-2 text-sm font-medium text-gray-700 hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-indigo-500 focus:ring-offset-2 disabled:opacity-50 dark:border-gray-600 dark:bg-gray-800 dark:text-gray-200 dark:hover:bg-gray-700"
+        >
+          下一页
+        </m.button>
+      </m.div>
+    </m.div>
+  )
+}
